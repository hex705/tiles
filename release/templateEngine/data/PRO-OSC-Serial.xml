<template>

<counter>$OSC_COUNT</counter>

<unique>
	<imports>
	import oscP5.*;
	</imports>
	
	<functions>
	void oscEvent(OscMessage incoming) {
		print(incoming);
	}
	</functions>
	
</unique>

<always></always>

<!-- overwritting the default --> 
<default>
	<variables>
	OscSerial oscSerial$OSC_COUNT;
	</variables>
	<setup>	
	oscSerial$OSC_COUNT = new OscSerial(this, proSerial$SERIAL_COUNT);
	</setup>
	<loop>
	OscMessage msg = new OscMessage("/outgoing");
	msg.add(mouseX);
	oscSerial$OSC_COUNT.send(msg);
	</loop>	 
</default>

</template>